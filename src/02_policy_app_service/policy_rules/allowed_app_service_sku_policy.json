{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Web/sites"
      },
      {
        "not": {
          "field": "Microsoft.Web/sites/sku",
          "in": "[parameters('listOfAllowedSku')]"
        }
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}
