{
  "listOfAllowedSku": {
    "type": "Array",
    "metadata": {
      "description": "The list of SKUs that can be specified for App Service Plans",
      "displayName": "Allowed SKUs"
    },
    "defaultValue": ["P1v3","WS1"]
  },
  "listOfAllowedKind": {
    "type": "Array",
    "metadata": {
      "description": "The list of Kind that can be specified for App Service Plans",
      "displayName": "Allowed Kind"
    },
    "defaultValue": ["linux", "elastic"]
  },
  "effect": {
    "type": "String",
    "metadata": {
      "displayName": "Effect",
      "description": "Enable or disable the execution of the policy"
    },
    "allowedValues": ["Audit", "Disabled"],
    "defaultValue": "Audit"
  }
}
